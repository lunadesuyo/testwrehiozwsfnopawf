{"_id":"zTqHrECSeyuHKTUE","name":"Purity of Body","type":"feat","img":"icons/magic/control/encase-creature-humanoid-hold.webp","data":{"description":{"value":"<p>At 10th level, your mastery of the ki flowing through you makes you immune to disease and poison.</p>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","width":null,"units":"","type":""},"range":{"value":"","long":null,"units":""},"uses":{"value":0,"max":0,"per":null,"type":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":"","value":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell","value":""},"requirements":"Monk 10","recharge":{"value":null,"charged":false}},"effects":[{"_id":"4fJTCpWEMkoPvkXK","changes":[{"key":"data.traits.ci.value","mode":2,"value":"poisoned","priority":"20"},{"key":"data.traits.ci.value","mode":2,"value":"diseased","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":"icons/magic/control/encase-creature-humanoid-hold.webp","label":"Purity of Body","origin":"Compendium.5e-complete.5e-class-features.zTqHrECSeyuHKTUE","transfer":true,"flags":{"core":{"statusId":""},"dae":{"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true},"dnd5e-helpers":{"rest-effect":"Ignore"}},"tint":null,"selectedKey":["data.traits.ci.value","data.traits.ci.value"]}],"folder":null,"sort":0,"permission":{"default":0,"LCw1mxC8S2Al3dWA":3},"flags":{"core":{"sourceId":"Compendium.world.class-features-expanded.bqWA7t9pDELbNRkp"},"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0","chargeType":"c1","destroy":false,"destroyFlavorText":"reaches 0 charges: it crumbles into ashes and is destroyed.","rechargeable":false,"recharge":"0","rechargeType":"t1","rechargeUnit":"r1","sorting":"l"},"betterRolls
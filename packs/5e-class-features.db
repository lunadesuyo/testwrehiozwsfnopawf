{"_id":"HiizEejyxUXmyToM","name":"Aura of Courage","type":"feat","img":"icons/magic/light/explosion-beam-impact-silhouette.webp","data":{"description":{"value":"<p>Starting at 10th level, you and friendly creatures within 10 feet of you can't be <strong>frightened </strong>while you are conscious.</p>\n<p>At 18th level, the range of this aura increases to 30 feet.</p>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":null,"type":"self"},"range":{"value":10,"long":30,"units":"ft"},"uses":{"value":0,"max":0,"per":"","type":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":"","value":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell","value":""},"requirements":"Paladin 10","recharge":{"value":null,"charged":false}},"effects":[{"_id":"eIhmjUqKvl29oC0Z","changes":[{"key":"data.traits.ci.value","mode":2,"value":"frightened","priority":"20"}],"disabled":false,"duration":{"startTime":null},"icon":"icons/magic/light/explosion-beam-impact-silhouette.webp","label":"Aura of Courage","origin":"Compendium.5e-complete.5e-class-features.HiizEejyxUXmyToM","transfer":true,"flags":{"core":{"statusId":""},"dae":{"stackable":"none","durationExpression":"","specialDuration":[],"transfer":true},"dnd5e-helpers":{"rest-effect":"Ignore"}},"tint":null,"selectedKey":"data.traits.ci.value"}],"folder":null,"sort":0,"permission":{"default":0,"LCw1mxC8S2Al3dWA":3},"flags":{"core":{"sourceId":"Compendium.world.class-features-expanded.nahSkBO6LH4HkpaT"},"betterRolls5e":{"quickDesc":{"value":true,"altValue":true},"quickProperties":{"value":true,"altValue":true},"quickOther":{"value":true,"altValue":true},"quickFlavor":{"value":true,"altValue":true}},"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0